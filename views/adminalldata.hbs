<div class="student" style="width: 100%;min-width:1100px">
          <div class="student-details">
            <ul class="details">
              <li class="topic">Name</li>
              {{#each Data}}
              <li><a href="#">{{this.name}}</a></li>
              {{/each}}
            </ul>
            <ul class="details">
              <li class="topic">PH No.</li>
                      {{#each Data}}
              <li><a href="#">{{this.phoneNumber}}</a></li>
              {{/each}}
            </ul>
            <ul class="details">
              <li class="topic">Date</li>
                      {{#each Data}}
                      <li>{{this.joindate}}</li>
                      {{/each}}
            </ul>
            <ul>
              <li class="topic">Address</li>
                {{#each Data}}
                  <li><a href="#">{{this.address}}</a></li>
                {{/each}}
            </ul>
            <ul class="details">
              <li class="topic">Delete</li>
                      {{#each Data}}
              <li><a href="/code123admin/delete/{{this._id}}" ><button style="display: flex;width:80px;height:40px;background-color:rgb(237, 58, 58);justify-content:center;align-content:center;border:1px solid transparent;border-radius:10px;color:aliceblue;padding:10px;">Delete</button></a></li>
              {{/each}}
            </ul>
          </div>
        </div>
</div>